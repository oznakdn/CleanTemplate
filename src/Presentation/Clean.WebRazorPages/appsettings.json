{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "BaseUrl": "http://localhost:5019/api/",
  "EndPoints": {
    "Auth": {
      "Register": "auth/register", // POST
      "Login": "auth/login", // PUT
      "CreateRole": "auth/createrole", // POST
      "GetRoles": "auth/getroles", // GET
      "AssignRole": "auth/assignrole" // PUT
    },
    "Account": {
      "Register": "account/register", // POST
      "Login": "account/login" // PUT
    },
    "Product": {
      "GetProducts": "products/getproducts", // GET
      "GetProductsCache": "products/getproductscache", // GET
      "GetProductDetail": "products/getproductdetail", // GET
      "GetProductsWithDataShaping": "products/getproductswithdatashaping", // GET
      "CreateProduct": "products/createproduct" // POST
    },
    "Customer": {
      "GetCustomer": "customers/getcustomer", // GET
      "GetCustomers": "customers/getcustomers", // GET
      "GetCustomerBasket": "customers/getcustomerbasket" // GET
    },
    "Basket": {
      "AddBasketItem": "baskets/addbasketitem", // POST
      "RemoveBasketItem": "baskets/removebasketitem", // PUT
      "UpdateBasketItem": "baskets/updatebasketitem" // PUT
    },
    "Order": {
      "CreateOrder": "orders/createorder", // POST
      "GetCustomerOrders": "orders/getcustomerorders" // GET
    }
  }
}
